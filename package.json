{
  "name": "sketch-find-and-replace",
  "description": "find-and-replace Plugin for sketch",
  "version": "3.0.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "homepage": "https://github.com/icai/Sketch-Find-And-Replace",
  "repository": {
    "type": "git",
    "url": "https://github.com/icai/Sketch-Find-And-Replace.git"
  },
  "bugs": {
    "url": "https://github.com/icai/Sketch-Find-And-Replace/issues"
  },
  "resources": [
    "./resources/index.js"
  ],
  "scripts": {
    "dev:ui": "vite --config vite.config.ts",
    "dev": "npm-run-all --parallel watch:plugin watch:ui",
    "watch:plugin": "vite build --watch --config vite.config.plugin.ts",
    "watch:ui": "vite build --watch --config vite.config.ts",
    "build:plugin": "vite build --config vite.config.plugin.ts",
    "build:ui": "vite build --config vite.config.ts",
    "build": "npm run build:plugin && npm run build:ui",
    "postinstall": "npm run build && npm run lnk",
    "lnk": "tsx scripts/link-plugin.ts",
    "unlink": "tsx scripts/unlink-plugin.ts",
    "remove-sketch": "tsx scripts/remove-sketch-plugin.ts",
    "update-appcast": "tsx scripts/update-appcast.ts"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.8",
    "node-html-parser": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "object-assign": "^4.1.1",
    "semver": "^7.7.2",
    "tsx": "^4.20.3",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.26",
    "xml2js": "^0.6.2"
  },
  "author": "Terry Cai <gidcai@gmail.com>",
  "dependencies": {
    "vue": "^3.4.21"
  }
}
